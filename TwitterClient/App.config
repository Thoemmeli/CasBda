<?xml version="1.0" encoding="utf-8"?>

<!-- Config file of CLI Twitter client -->

<configuration>
  <appSettings>
    <!-- New functionality
        | (pipe) seperates Search Groups
        , (comma) seperates Search Terms
    -->
    <!-- All Suchworte von Hagen, Alle Produkte von Roche (https://compendium.ch/comp/de), Alle Produkte von Novartis (https://compendium.ch/comp/de)-->
    <add key="twitter_keywords" value="pharma,Roche,Novartis,diphenhydramine,influenza,vaccine,flu shot,dextroamphetamine,codeine,morphine,ibuprofen,alprazolam,paracetamol/acetaminophen,oxycodone,zolpidem,alemtuzumab,lemtrada,teriflunomide,aubagio,interferon beta-1a,avonex,rebif,plegridy,interferon beta-1b,betaferon,betaseron,ferona,extavia,compesk,glatiramer acetate,copaxone,glatopa,daclizumab,dimethyl fumarate,tecfidera,fingolimod,gilenya,natalizumab,tysabri,pain,altered state of consciousness,headache,malaise,drug ineffective,nonspecific reaction,weitght increase,death,anxiety,fatigue,memory impairment,skin discomfort,infection,depression,blood glucose abnormal,injury,weight decreased,pyrexia,blood pressure increased,overdose,drug abuse,dependence,insomnia,abdominal pain,pregnancy,dyspnea,affect lability,injection site pain,asthma,swelling,stress,abnormal dreams,convulsions,cough,arthralgia,condition aggravated,fall ,influenza-like illness,somnolence,incorrect dose administered,hepatic function abnormal,pregnancy,multiple sclerosis,flu,grippe,headache,kopfschmerzen,neocitran,supradyn,multi-vitamin,husten,cough,paracetamol,asperin,vitamine C,zink,theraflu,acetaminophen,night nurse,beechams,lemsip,ibuprofen,panadol antigrippine,pretuval,vicks medi nait,acetalgin,xylo-mepha,fluimucil,Dextromethorphan,Actemra,Alecensa,Avastin,Bactrim,Cellcept,Copegus,Cotellic,Cymevene,Dormicum,Erivedge,Esbriet,Fuzeon,Gazyvaro,Herceptin,Inhibace,Inhibace plus,Invirase,Kadcyla,Konakion,Kytril,Lexotanil,Mabthera,Madopar,Mircera,NeoRecormon,Ocrevus,Pegasys,Perjeta,Pulmozyme,Recormon,Rivotril,Roaccutan,Rocaltrol,Rocephin,Roferon-A,Tamiflu,Tarceva,Tecentriq,Valcyte,Valium,Xeloda,Xenical,Zelboraf,Aclasta,Afinitor,Aimovig,Anafranil,Arzerra,Azarga,Azopt,Betoptic,Certican,Cibacen,Ciloxan,Ciproxin,Co-Diovan,Cosentyx,Desferal,Diovan,Duotrav,Emadine,Entresto,Estalis,Estradot,Exelon,Exforge,Ejade,Famvir,Farydak,Femara,Focalin,Galvumet,Galvus,Glivec,Hycamtin,Ilaris,Iopidine,Jadenu,Jakavi,Kisqali,Kymriah,Lamisil,Lescol,Lioresal,Lucentis,Maxidex,Maxitrol,Mekinist,Methergin,Moflonide,Myfortic,Nevanac,Nitroderm,Onbrez Breezhaler,Opatanol,Parlodel,Proleukin,Revolade,Riamet,Ritalin,Rydapt,Syndimmun,Sandostatin,Sebivo,Seebri,Signifor,Simbrinza,Simulect,Sirdalud,Stalevo,Starlix,Symmetrel,Tafinlar,Tasigna,Tegretol,Tobi,Tobradex,Tobrex,Travatan,Triesence,Trileptal,Tyverb,Ultibro,Vigamox,Visudyne,Voltaren,Voltfast,Votrient,Votubia,Xolair,Zaditen,Zofran,Zometa,Zykadia" />
    <add key="create_big_file" value="true" />

    <!-- Twitter Streaming API settings.  Replace Twitter OAuth settings with your own -->
    <add key="oauth_consumer_key" value="" />
    <add key="oauth_consumer_secret" value="" />
    <add key="oauth_token" value="-" />
    <add key="oauth_token_secret" value="" />

    <!---Mark Rowe - Extended Functionality-->
    <!--If Sentiment Can't figure it out.. kill it from data -->
    <add key="clear_all_with_undefined_sentiment" value="true" />
    <!--Extended Information adds "Tweet Text" and "Author"-->
    <add key="send_extended_information" value="true" />
    <!--Match Mode
         Valid Values:
         "Any"     // must match any keyword.
         "Measure" // -> TODO: Same as Any but calculates how many. 
         "All"     // must match all keywords in the Search Group
    -->
     <add key="match_mode" value="any" />
    <!-- Turn false for Troubleshooting, while false will not send to event hub-->
    <add key="AzureOn" value="false" />
    
    <!-- Service Bus specific app setings for messaging connections -->
    <add key="EventHubConnectionString" value="Access Token" />
    <add key="EventHubName" value="Access Token Secret" />
  </appSettings>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Data.OData" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.8.4.0" newVersion="5.8.4.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

</configuration>
